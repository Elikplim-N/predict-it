{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "api/auth.py"
    },
    {
      "src": "/api/submit",
      "dest": "api/submit.py"
    },
    {
      "src": "/api/submissions",
      "dest": "api/submit.py"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "api/admin.py"
    }
  ]
}
